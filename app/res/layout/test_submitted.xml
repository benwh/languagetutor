<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical" >

	<LinearLayout style="@style/header_content_layout" >

		<TextView
			style="@style/title_header_text"
			android:text="@string/test_com" />
	</LinearLayout>

	<LinearLayout style="@style/main_content_layout" >

		<TextView
			android:id="@+id/test_submitted"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center_horizontal|center_vertical"
			android:text="@string/test_submitted" />
	</LinearLayout>

</LinearLayout>